{
  "applications": {
    "gecko": {
      "id": "easyscreenshot@mozillaonline.com",
      "strict_min_version": "55.0"
    }
  },
  "background": {
    "scripts": [
      "lib/Chaz.js",
      "editor/editor.background.js",
      "background/PrintScreenLauncher.background.js",
      "background/easyscreenshot.background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "images/browser_action-16.png",
      "32": "images/browser_action-32.png"
    },
    "default_popup": "popup/easyscreenshot.popup.html",
    "default_title": "__MSG_browserActionTitle__"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "lib/mococn.css"
      ],
      "js": [
        "lib/Chaz.js",
        "lib/script.js",
        "content/PrintScreen.content.js",
        "content/easyscreenshot.content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en_US",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "http://www.firefox.com.cn/",
  "icons": {
    "48": "images/icon-48.png",
    "64": "images/icon-64.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "permissions": [
    "<all_urls>",
    "downloads",
    "notifications",
    "storage"
  ],
  "version": "2.55.3"
}